# Gonad NFT - Development Environment Variables
# Copy this file to .env and customize the values

# Database Configuration
DATABASE_URL="file:./data/dev.db"

# WalletConnect Configuration
NEXT_PUBLIC_WC_PROJECT_ID="your-wallet-connect-project-id-here"

# JWT Authentication
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"

# NextAuth Configuration (if used in future)
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret-change-this-too"

# Application Environment
NODE_ENV="development"

# Optional: Prisma Studio Configuration
# PRISMA_STUDIO_PORT=5555

# Optional: Additional Configuration
# PORT=3000
# HOSTNAME="0.0.0.0"
# NEXT_TELEMETRY_DISABLED=1
